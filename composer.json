{
    "name": "lag/colonel-smoker-bundle",
    "description": "The Colonel helps you to check the smoke in your application",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Arnaud Fr√©zet",
            "email": "arnaudfrezet@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "LAG\\SmokerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LAG\\SmokerBundle\\Tests\\": "tests/SmokerBundle/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "require": {
        "php": "^7.2",
        "ext-dom": "*",
        "doctrine/doctrine-bundle": "^1.9 || ^2.0",
        "doctrine/orm": "^2.6",
        "fabpot/goutte": "^3.2",
        "symfony/config": "^4.1",
        "symfony/console": "^4.1",
        "symfony/dependency-injection": "^4.1",
        "symfony/filesystem": "^4.1",
        "symfony/http-kernel": "^4.1",
        "symfony/options-resolver": "^4.1",
        "symfony/property-access": "^4.1",
        "symfony/routing": "^4.1",
        "symfony/yaml": "^4.1",
        "twig/twig": "^2.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpstan/phpstan": "^0.12.82",
        "phpunit/phpunit": "^7.4",
        "symfony/debug": "^4.2"
    }
}
