<html>
<head>
    <title>Smoker Tests Results</title>
    <link rel="stylesheet"
          href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css"
          integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO"
          crossorigin="anonymous"/>
</head>
<body>
    <div class="container">
        <div class="row">
            <div class="col-12">
                <table class="table">
                    <thead>
                    <tr>
                        <th>Url</th>
                        <th>Status</th>
                    </tr>
                    </thead>
                    <tbody>
                    {% for data in successData %}
                        <tr>
                            <td>
                                {{ data.url }}
                            </td>
                            <td>
                                {{ data.responseCode }}
                            </td>
                        </tr>
                    {% endfor %}
                    </tbody>
                </table>

                <br><br>

                <table class="table">
                    <thead>
                        <tr>
                            <th>Url</th>
                            <th>Status</th>
                            <th>Handler</th>
                            <th>Message</th>
                        </tr>
                    </thead>
                    <tbody>
                    {% for data in errorData %}
                        <tr>
                            <td>
                                {{ data.url }}
                            </td>
                            <td>
                                {{ data.responseCode }}
                            </td>
                            <td>
                                {{ data.handler }}
                            </td>
                            <td>
                                {{ data.message }}
                            </td>
                        </tr>
                    {% endfor %}
                    </tbody>
                </table>

            </div>
        </div>
    </div>
</body>
</html>
